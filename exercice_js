        const a = 1;
        const b = 2;
        const c = 1;

        if (a === b && a=== c && b===c) {
            console.log("The three variables are the same")
        }
        
        else if (a === b || a === c || b === c) {
            console.log("Two of the variables are the same")
        }

        else if (a !== b && a !== c && b !== c) { 
            console.log("all variables are different")
        }
